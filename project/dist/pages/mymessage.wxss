
.row {
  display: flex;
  flex-direction: row;
  justify-content: center;
}
.column {
  flex-direction: column;
  justify-content: flex-start;
}
.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.underline {
  height: 1px;
  width: 100%;
  margin-left: 16%;
  background-color: #cdcdcd;
}
