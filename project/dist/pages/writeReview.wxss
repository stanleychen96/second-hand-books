@import "./../components/leadtable.wxss";

.global {
  display: flex;
  background-size: 100% 100%;
  height: 100%;
  width: 100%;
  background-color: #DEDEDE;
  /* position: fixed; */
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.line {
  border-bottom: 1px solid 
}
.row {
display: flex;
flex-direction: row;
justify-content: center;
}
.column {
  flex-direction: column;
  justify-content: flex-start;
}
.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
}
.underline {
  height: 1px;
  width: 100%;
  margin-left: 16%;
  background-color: #cdcdcd;
}
